{
  "name": "n8n-ai-assistant",
  "version": "0.1.0",
  "description": "AI-powered Chrome/Edge extension for n8n workflow assistance",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "dev:setup": "./scripts/dev-setup.sh",
    "build:extension": "./scripts/build-extension.sh",
    "test:n8n": "./scripts/test-n8n.sh",
    "clean": "rm -rf dist/",
    "watch": "vite build --watch",
    "watch:script": "./scripts/watch.sh",
    "dev:watch": "vite --watch"
  },
  "dependencies": {
    "@langchain/core": "^0.3.0",
    "@langchain/openai": "^0.3.0",
    "langchain": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "webextension-polyfill": "^0.10.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/chrome": "^0.0.270",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "typescript": "^5.5.4",
    "vite": "^6.0.1"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "yarn@1.22.22"
}
